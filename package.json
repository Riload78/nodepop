{
  "name": "nodepop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production ./bin/www",
    "dev": "NODE_ENV=development DEBUG=nodepop:* nodemon ./bin/www",
    "initDB": "node ./lib/initDB.js",
    "lint": "npx eslint --fix",
    "test": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles --silent",
    "test:watch": "NODE_ENV=test jest --verbose --silent --watchAll",
